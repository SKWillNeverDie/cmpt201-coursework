#include <stdio.h>
#include <sys/types.h>
#include <unistd.h>
int main(int arg_count, char* args[]) {
  printf("YAY!\n");
  for(int i=0; i<arg_count; i++){
    printf("arg[%d]=%s\n", i, args[i]);
  }
  pid_t pid = fork();
  if (pid == 0) {
    // I am the child
    printf("CHILD!\n");
    char *args[]={"/bin/ls", "-a", "-l", "-h", NULL};
    excev("/bin/ls",args);
  } else {
    printf("PARENT!\n");
    execl("/bin/ls", "/bin/ls", "-a", NULL);
  }
  printf("end\n");
}
